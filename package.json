{
  "name": "dcautobot",
  "version": "1.0.0",
  "description": "Discord 自動發話機器人",
  "main": "bot/server.js",
  "scripts": {
    "start": "node bot/server.js",
    "dev": "node bot/server.js",
    "bot": "node bot/bot.js",
    "selfbot": "node selfbot/selfbot.js",
    "check-secrets": "node scripts/check-secrets.js",
    "prepare": "husky install",
    "start:pm2": "npx pm2 start ecosystem.config.js --only dcautobot",
    "stop:pm2": "npx pm2 stop dcautobot || true",
    "restart:pm2": "npx pm2 restart dcautobot || npx pm2 start ecosystem.config.js --only dcautobot"
  },
  "keywords": [
    "discord",
    "bot",
    "auto-message"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "discord.js-selfbot-v13": "^3.7.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2"
  }
  ,
  "devDependencies": {
    "husky": "^8.0.0",
    "pm2": "^5.2.0"
  }
}
